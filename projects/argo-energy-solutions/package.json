{
  "name": "argo-energy-solutions",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "analyze:energy": "node scripts/analyze-energy-data.js",
    "explore:channels": "node scripts/explore-channels.js",
    "analyze:wilson": "node scripts/wilson-center-analysis.js",
    "unit:health": "node scripts/unit-health-report.js",
    "diagnose:data": "node scripts/diagnose-data-access.js",
    "check:daily": "bash scripts/check-data-daily.sh",
    "export:csv": "node scripts/export-to-csv.js",
    "ingest:data": "node scripts/ingest-eniscope-data.js",
    "ingest:full": "node scripts/ingest-eniscope-data.js --full",
    "ingest:incremental": "node scripts/ingest-eniscope-data.js --incremental",
    "db:check": "node scripts/check-database.js",
    "api:server": "node server/api-server.js",
    "api:dev": "npx nodemon server/api-server.js",
    "dev:all": "concurrently \"npm run api:server\" \"npm run dev\"",
    "start": "npm run dev:all"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.14.2",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "recharts": "^2.10.3",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^25.0.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "tsx": "^4.21.0",
    "typescript": "^5.2.2",
    "vite": "^7.3.1"
  }
}
